{
  "name": "openmud",
  "version": "1.0.5",
  "description": "OpenMud Client",
  "homepage": "https://openmud.github.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenMud/client.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild --sourcemap",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@excaliburjs/testing": "0.25.1",
    "@microsoft/signalr": "^7.0.11",
    "@mui/material": "^5.14.11",
    "excalibur": "^0.28.1",
    "react": "^18.2.0",
    "react-command-palette": "^0.22.1",
    "react-dom": "^18.2.0",
    "sweetalert2-react-content": "^5.0.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.22",
    "@antfu/ni": "^0.21.8",
    "@antfu/utils": "^0.7.6",
    "@types/node": "^20.8.3",
    "@types/react-command-palette": "^0.18.2",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "esno": "^0.17.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.8.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.11"
  }
}
